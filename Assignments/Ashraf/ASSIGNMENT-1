#include <iostream>
using namespace std;
int main() {
  int n;
  cin>>n;
  int row=0;
  int row1=0;
  while(row<n){
       int j=0;
       while(j<n-row){ 
           cout<<j+1;
           j++;
           
       }
       if(row>=1){
        int star = 0;
        while (star < 2 * row) {
            cout << "*";
            star++;
        }
       }
       while(j>=1){
           cout<<j;
           j--;
       }
       cout<<endl;
       row++;
      }
     
    return 0;
}
